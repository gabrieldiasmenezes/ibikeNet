  "components": {
    "schemas": {
      "Administrador": {
        "required": [
          "email",
          "nome",
          "password",
          "patioId",
          "status"
        ],
        "type": "object",
        "properties": {
          "cpf": {
            "maxLength": 11,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "nome": {
            "minLength": 1,
            "type": "string"
          },

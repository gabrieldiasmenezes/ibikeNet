          }
        },
        "additionalProperties": false
      },
      "Moto": {
        "required": [
          "dataUltimoCheck",
          "kmAtual",
          "modelo",
          "status"
        ],
        "type": "object",
        "properties": {
          "placa": {
            "maxLength": 10,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "modelo": {
            "minLength": 1,
            "type": "string"
          },
          "status": {
            "$ref": "#/components/schemas/StatusMoto"
          },
          "kmAtual": {
            "type": "number",
            "format": "double"
          },
          "dataUltimoCheck": {
            "type": "string",
            "format": "date-time"
          },
          "patioId": {
            "type": "integer",
            "format": "int32"
          },
          "patio": {
            "$ref": "#/components/schemas/Patio"
          }
        },
        "additionalProperties": false
      },
      "MotoPagedResult": {
        "type": "object",
        "properties": {
          "items": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Moto"
            },
            "nullable": true
          },
          "totalCount": {
            "type": "integer",
            "format": "int32"
          },
          "pageNumber": {
            "type": "integer",
            "format": "int32"
          },
          "pageSize": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "Patio": {
        "required": [
          "capacidade",
          "nome",
          "status"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64"
          },
          "nome": {
            "minLength": 1,
            "type": "string"
          },
          "capacidade": {
            "type": "integer",
            "format": "int32"
          },
          "status": {
            "$ref": "#/components/schemas/StatusPatio"
          },
          "administradores": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Administrador"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
